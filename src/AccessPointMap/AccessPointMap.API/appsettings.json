{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": ".\\Logs\\log.txt",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}"
        }
      }
    ]
  },
  "DatabaseSettings": {
    "ConnectionString": ""
  },
  "JsonWebTokenSettings": {
    "TokenSecret": "",
    "BearerTokenExpirationMinutes": 0,
    "RefreshTokenExpirationDays": 0
  },
  "AllowedHosts": "*"
}
